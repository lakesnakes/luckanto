            IUniswapV2Pair(swapA).swap(0, ret, address(msg.sender), bytes(""));
            
        }else{
            ret = getAmountOut(ineth, r1, r0);
            IUniswapV2Pair(swapA).swap(ret, 0, address(msg.sender), bytes(""));
            
        }
        
    }

